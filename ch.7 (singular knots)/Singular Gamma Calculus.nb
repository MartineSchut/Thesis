       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"s", " ", "t"}]}], ")"}], " ", 
        SubscriptBox["c", "b"], " ", 
        SubscriptBox["r", "a"]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "s"}], ")"}], " ", 
        SubscriptBox["c", "a"], " ", 
        SubscriptBox["r", "b"]}], "+", 
       RowBox[{"s", " ", "t", " ", 
        SubscriptBox["c", "b"], " ", 
        SubscriptBox["r", "b"]}]}]}], "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Definition", " ", "of", " ", "the", " ", 
     RowBox[{"stitching", " ", ":"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     SubscriptBox["m", 
      RowBox[{"a_", ",", 
       RowBox[{"b_", "\[Rule]", "k_"}]}]], "[", 
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]_", ",", "A_"}], "]"}], "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[CapitalGamma]", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[Mu]", "=", 
           RowBox[{"1", "-", 
            RowBox[{
             SubscriptBox["\[PartialD]", 
              SubscriptBox["r", "a"]], 
             RowBox[{
              SubscriptBox["\[PartialD]", 
               SubscriptBox["c", "b"]], "A"}]}]}]}], ")"}], "\[Omega]"}], ",", 
        RowBox[{"A", "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[PartialD]", 
             SubscriptBox["r", "a"]], "A"}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             SubscriptBox["\[PartialD]", 
              SubscriptBox["c", "b"]], "A"}], ")"}], "/", "\[Mu]"}]}]}]}], 
       "]"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["c", "b"], "|", 
           SubscriptBox["r", "a"]}], ")"}], "\[Rule]", "0"}], ",", 
        RowBox[{
         SubscriptBox["c", "a"], "\[Rule]", 
