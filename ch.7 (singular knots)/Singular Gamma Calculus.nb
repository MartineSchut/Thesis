Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Fromatting", " ", "and", " ", "Multiplication", " ", 
    RowBox[{"rules", ":"}]}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{"\[CapitalGamma]", "/:", 
    RowBox[{
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]1_", ",", "A1_"}], "]"}], 
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]2_", ",", "A2_"}], "]"}]}], ":=", 
    RowBox[{"\[CapitalGamma]", "[", 
     RowBox[{
      RowBox[{"\[Omega]1", " ", "\[Omega]2"}], ",", 
      RowBox[{"A1", "+", " ", "A2"}]}], "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"\[CapitalGamma]", "/:", 
    RowBox[{
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]1_", ",", "A1_"}], "]"}], "\[Congruent]", 
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]2_", ",", "A2_"}], "]"}]}], ":=", 
    RowBox[{"Simplify", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Omega]1", " ", "-", "\[Omega]2"}], "\[Equal]", "0"}], "&&", 
      
      RowBox[{
       RowBox[{"A1", "-", " ", "A2"}], "\[Equal]", "0"}]}], "]"}]}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[CapitalGamma]Collect", "[", 
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]_", ",", "A_"}], "]"}], "]"}], ":=", 
    RowBox[{"\[CapitalGamma]", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Simplify", "[", "\[Omega]", "]"}], "//", "Expand"}], ",", 
      RowBox[{"Collect", "[", 
       RowBox[{"A", ",", 
        SubscriptBox["r", "_"], ",", 
        RowBox[{
         RowBox[{"Collect", "[", 
          RowBox[{"#", ",", 
           SubscriptBox["c", "_"], ",", "Factor"}], "]"}], "&"}]}], "]"}]}], 
     "]"}]}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[CapitalGamma]Format", "[", 
     RowBox[{"\[CapitalGamma]", "[", 
      RowBox[{"\[Omega]_", ",", "A_"}], "]"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"S", ",", "M"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"S", "=", 
        RowBox[{"Union", "@", 
         RowBox[{"Cases", "[", 
          RowBox[{
           RowBox[{"\[CapitalGamma]", "[", 
            RowBox[{"\[Omega]", ",", "A"}], "]"}], ",", 
           RowBox[{
            SubscriptBox[
             RowBox[{"(", 
              RowBox[{"r", "|", "c"}], ")"}], "a_"], "\[RuleDelayed]", " ", 
            "a"}], ",", "Infinity"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"M", "=", 
        RowBox[{"Outer", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Factor", "[", 
            RowBox[{
             SubscriptBox["\[PartialD]", 
              RowBox[{
               SubscriptBox["c", "#1"], ",", 
               SubscriptBox["r", "#2"]}]], "A"}], "]"}], "&"}], ",", "S", ",",
           "S"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"M", "=", 
        RowBox[{
         RowBox[{"Prepend", "[", 
          RowBox[{"M", ",", 
           RowBox[{
            RowBox[{
             SubscriptBox["r", "#"], "&"}], "/@", "S"}]}], "]"}], "//", 
         "Transpose"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"M", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{"M", ",", 
          RowBox[{"Prepend", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              SubscriptBox["c", "#"], "&"}], "/@", "S"}], ",", "\[Omega]"}], 
           "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"M", "//", "MatrixForm"}]}]}], "\[IndentingNewLine]", "]"}]}], 
   "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"The", " ", "positivie"}], ",", " ", "negative", ",", " ", 
     RowBox[{"and", " ", "singular", " ", 
      RowBox[{"crossings", " ", ":"}]}]}], " ", "*)"}], "\n", 
